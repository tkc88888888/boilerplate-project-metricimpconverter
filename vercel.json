{
  "version": 2,
  "builds": [
    {
      "src":"./server.js",
      "use":"@vercel/node"
    },
    {
      "src":"./views/**",
      "use":"@vercel/static"
    },
    {
      "src":"./controllers/**",
      "use":"@vercel/node"
    },
    {
      "src":"./routes/**",
      "use":"@vercel/node"
    },
    {
      "src":"./tests/**",
      "use":"@vercel/static"
    },
    {
      "src":"./public/**",
      "use":"@vercel/static"
    }
  ],
  "routes": [
    {
      "src":"/(.*)",
      "dest":"./server.js"
    },
    {
      "src":"/(.*)",
      "dest":"./test-runner.js"
    },
    {
      "src":"/",
      "dest":"./controllers/convertHandler.js"
    },
    {
      "src": "/(.+)",
      "dest": "routes/$1"
    },
    {
      "src": "/(.+)",
      "dest": "routes/$1"
    },
    {
      "src": "/(.+)",
      "dest": "tests/$1"
    },
    {
      "src": "/(.+)",
      "dest": "public/$1"
    }
  ]
}
