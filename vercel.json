{
  "version": 2,
  "builds": [
    {
      "src":"./server.js",
      "use":"@vercel/node"
    },
    {
      "src":"./views/**",
      "use":"@vercel/static"
    },
    {
      "src":"./public/**",
      "use":"@vercel/static"
    }
  ],
  "routes": [
    {
      "src":"/(.*)",
      "dest":"./server.js"
    },
    {
      "src":"/",
      "dest":"./views/index.html"
    },
    {
      "src": "/(.+)",
      "dest": "public/$1"
    }
  ]
}
